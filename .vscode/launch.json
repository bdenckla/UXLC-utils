            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "generate WLC a-note change proposals",
            "type": "debugpy",
            "request": "launch",
            "program": "py/main_wlc_a_notes.py",
            "console": "integratedTerminal"
        },
        {
            "name": "estimate atom location",
            "type": "debugpy",
            "request": "launch",
            "program": "py/main_uxlc_estimate_atom_loc.py",
            "args": ["Genesis", "24", "24", "3"],
            "console": "integratedTerminal"
        },
        {
            "name": "download changes",
            "type": "debugpy",
            "request": "launch",
            "program": "py/main_uxlc_download_changes.py",
            "console": "integratedTerminal"
        },
        {
            "name": "check changes",
            "type": "debugpy",
            "request": "launch",
            "program": "py/main_uxlc_check_changes.py",
            "console": "integratedTerminal"
        }
    ]
